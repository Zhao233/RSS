/* pages/pay/pay.wxss */
.pay{background: #fff; text-align: center; padding-top: 14rpx;}
.pay-time{padding: 14rpx 0 22rpx; border-bottom: 1rpx solid #e6e6e6; display: flex; align-items: center; justify-content: center;}
.pay-text{font-size: 22rpx; color: #808080; display: block;}
.time-list{display: flex}
.time-item{width: 28rpx; height: 28rpx; line-height: 28rpx; text-align: center; background: #656467; font-size: 24rpx; color: #fff; margin: 0 2rpx;border-radius: 2rpx;}
.time-list>text{display: inline-block; width: 30rpx; height: 30rpx; line-height: 30rpx; text-align: center;}
.pay-message{height: 144rpx; text-align: left; padding: 0 52rpx; display: flex; align-items: center; justify-content: flex-start; position: relative;background-image: url(data:image/png;base64,);}
.pay-message{background-size: 120%;}
.pay-message image{position: absolute; left: 0; top: 0; width: 100%; height: 100%;}
.pay-message-left{width: 72rpx; height: 72rpx; overflow: hidden; border-radius: 50%; background: #ccc; margin-right: 46rpx;position: relative;}
.pay-price{font-size: 38rpx; color: #333;height: 38rpx;line-height: 38rpx;}
.pay-price text{position: relative;display: inline-block;max-width: 500rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.pay-price text::after{content:' ';display: block;width:100%;position: absolute;bottom: 0; border-bottom: 1px dashed #979797;}
.pay-address{margin-top:1rpx;font-size: 22rpx;display: inline-block;position: relative;height: 24rpx;line-height: 22rpx;margin-top: -10rpx;max-width: 500rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.pay-address::after{content:' ';display: block;width:100%;position: absolute;bottom: 0; border-bottom: 1px dashed #979797;}
.pay-icon{width: 42rpx; height: 42rpx; overflow: hidden; margin-right: 40rpx;}
.pay-icon image{width: 100%; height: 100%;}
.pay-method{height: 108rpx; padding: 0 28rpx; margin-top: 26rpx; background: #fff; display: flex; align-items: center;}
.pay-method-name{font-size: 28rpx; flex: 1;}
.pay-method .active{border: none;}
.pay-check{width: 40rpx; height: 40rpx; border: 1px solid #e6e6e6; border-radius: 50%; box-sizing: border-box;}
.pay-check image{width: 100%; height: 100%;}
.btn-group{text-align: center; margin-top: 60rpx;}
.btn-group button{width: 670rpx; display: flex; align-items: center; justify-content: center; height: 94rpx; background: #fd711c; color: #fff; font-size: 32rpx;}
.btn-group button .big-text{font-size: 48rpx;margin-left: 32rpx;}
button::after{content: none;}
button[disabled]{background-color:#fd711c !important;color: #fff !important;opacity: .4; }