page{min-height: 100%;position: relative;box-sizing: border-box;padding-bottom: 100rpx;}
.order-confirm-wrapper{width:100%;overflow:hidden;min-height: 100%;padding-bottom: 20rpx;}
.order-address{padding:0 0 0 24rpx;background:#fff;margin-bottom: 20rpx;}
.address-wrapper{display:flex;align-items:center;justify-content:space-between;height:144rpx;width:100%;border-bottom:0.5px solid #e6e6e6;box-sizing:border-box}
.address-detail{font-size: 28rpx;color: #333;}
.address-detail view{font-size:28rpx;color:#333}
.order-address .icon-arrow-right{margin-right:10rpx}

.user-name{margin-top:12rpx}

.dilivery-time{font-size:28rpx;height:86rpx;display:flex;justify-content:space-between;align-items:center;line-height:86rpx}
.dilivery-time .title{height:100%;color: #333;}
.dilivery-time .icon{margin-top: -1rpx;}
.choose-time{height:100%;color:#ff8162;display: flex;align-items: center;}
.order-detail-wrapper{background:#fff;padding-left:24rpx;}
.detail-header{height:88rpx;padding-left:0rpx;font-size:28rpx;line-height:88rpx;color:#999;border-bottom:0.5px solid #e6e6e6;box-sizing:border-box}
.goods-list{padding-right:24rpx;border-bottom:0.5px solid #e6e6e6}
.goods-list .item{padding:15rpx 0}
.primary-content{display:flex;justify-content:space-between;align-items:center;font-size:28rpx;color:#333}
.primary-content view{display:inline-block}
.primary-content .name{flex: 0 0 400rpx;color: #333;}
.primary-content .num{color: #999999;}
.primary-content .price{flex: 0 0 100rpx;text-align: right;}
.add-content{font-size:20rpx;color:#999;margin-top:3rpx}
.common-price,.dilivery-price,.super-price{display:flex;justify-content:space-between;padding:15rpx 24rpx 15rpx 0;font-size:28rpx;color: #333;}
.common-price{color:#fd711c}
.info{padding-top:14rpx;padding-left:24rpx;height:66rpx;font-size:24rpx;color:#f49130;box-sizing: border-box;}
.zhekou{background:#fff;padding-left:24rpx;}
.zhekou .title{color: #333;}
.vipcard,.youhuiquan{display:flex;justify-content:space-between;align-items:center;height:86rpx;line-height:86rpx;font-size:28rpx;padding-right:12rpx;border-bottom:0.5px solid #e6e6e6;box-sizing:border-box}

.youhuiquan .cont{color:#fd711c;height: 100%;line-height: 86rpx;display: flex;align-items: center;}
.vipcard .cont{color: #999999;}
.vipcard view{height: 100%;line-height: 86rpx;}
.vipcard switch{display: inline;vertical-align: middle;}
.vipcard .cont {display: flex;align-items: center;}
.vipcard .switch{width: 80rpx;height: 44rpx;display: flex;margin-left: 24rpx;}
.vipcard .switch image{width: 100%;height: 100%;margin-left: -14rpx;}
.xiaoji{height:86rpx;text-align:right;padding-right:24rpx;line-height:28rpx;font-size:28rpx}
.xiaoji text{height: 100%;display: inline-block;}
.orange{color:#fd711c!important}
.remark-box .title{color: #333;}
.remark{box-sizing:border-box;-webkit-box-sizing:border-box;width:100%;background:#fff;position: relative;margin-bottom: 40rpx;margin-top: 20rpx;font-size: 28rpx;padding: 25rpx 25rpx 25rpx 32rpx;}
.remark-text{float:left;font-size: 28rpx;color: #333;}
.remark textarea{height: 160rpx; width: 100%;position: static}
.remark .font-size{position: absolute; bottom: 20rpx; right: 10rpx; color: #ccc;}
.icon-info{margin-top: -1rpx;}
.footer{padding-left:24rpx;background:#fff;margin-top:40rpx;height:100rpx;display:flex;justify-content:space-between;align-items:center;position: fixed;bottom: 0;width: 100%;color: #333;}
.footer{position: fixed;bottom: 0;width: 100%;box-sizing: border-box;overflow: hidden;z-index: 20;}
.footer text:first-child{font-size:28rpx;line-height:100rpx;color:#333}
.footer text:nth-child(2){font-size:32rpx;display: inline-block;vertical-align: middle;height: 100%;line-height: 4rpx;}
.footer text:nth-child(2) text{display: inline-block;vertical-align: middle;line-height: 32rpx;}
.footer button{display:inline-block;width:222rpx;height:100%;line-height:100rpx;margin:0;background:#fd711c;color:#fff;border-radius:0}
button[disabled]{background: #fd711c !important;color: #fff !important;opacity: .4;}

.time-choose{position: fixed;top: 0;bottom: 0;left: 0;right: 0;z-index: 20;}
.mask{width: 100%;height: 100%;background: rgba(0,0,0,.5);}
.time-choose .wrapper{position: absolute;bottom: 0;height: 600rpx;width: 100%;}
.time-choose .content{display: flex;height: 510rpx;}
.day-list{flex: 0 0 224rpx;background: #fafafa;overflow-y: scroll;height: 100%;}
.day-list view{height: 90rpx;line-height: 90rpx;text-align: center;font-size: 28rpx;color: #333;}
.hour-list{flex: 1;background: #fff;overflow-y: scroll;height: 100%;}
.hour-list view{height: 90rpx;line-height: 90rpx;color: #666666;font-size: 28rpx;box-sizing: border-box;padding-left: 30rpx;background-repeat: no-repeat;background-size: 30rpx 30rpx;background-position: 458rpx 50%;background-image: url(data:image/png;base64,)}
.time-choose .btn{height: 90rpx;}
.time-choose .btn button{font-size: 36rpx;color: #333;box-sizing: border-box;border-top: 0.5px solid #e6e6e6;border-radius: 0}
.time-choose .btn button::after{content: '';border: none;}
.day-selected{background: #fff;}
.icon-arrow-right{height:24rpx;background-position:50% 50%;background:url(data:image/png;base64,) no-repeat center center;background-size:contain;}
.icon-info{line-height:33rpx;width:26rpx;height:24rpx;background:url(data:image/png;base64,) no-repeat;vertical-align:middle;background-size:24rpx 24rpx;}
.icon-tejia{margin-top:-3rpx;vertical-align:middle;background:url(data:image/png;base64,) no-repeat;background-size:cover;width:48rpx;height:28rpx}
.hour-selected{
    color: #fd711c !important;
    background-image: url(data:image/png;base64,) !important;}

button::after{content: none;}
.classname{height: 4rpx;width: 100%;display: flex;margin-top: 20rpx;}
.line-2px{width: 100%;height: 4rpx;}

.remark-box{background: #fff;padding: 0 18rpx 0 24rpx;height: 86rpx;line-height: 86rpx;margin-top: 20rpx;font-size: 28rpx;}
.remark-box .title{float: left;height: 86rpx;line-height: 86rpx;width: 80rpx;}
.remark-c{float: right;width: 600rpx;height: 86rpx;line-height: 86rpx;text-align: right;}
.remark-c>.content{float: right;width: 550rpx;color: #999999;}
.remark-c>.active{color: #666666;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.remark-c>.icon{float: right;margin-top: 32rpx;margin-right: -6rpx;}


.youhuiquan{padding-right: 10rpx;}
.icon-info-box{width: 22rpx;height: 22rpx;display: inline-block;}
.icon-info-box image{width: 100%;height: 100%;}
.info-box{display: flex;align-items: center;}
.info{position: relative;}
.icon-info-box{position: absolute;top: 10rpx;left: 24rpx;}
.info-box{position: absolute;top: 8rpx;left: 54rpx;}

 .icon-box{width: 30rpx;height: 30rpx;display: inline-block;vertical-align: middle;margin-left: 8rpx;}
 .icon-box image{width: 100%;height: 100%;}
 .icon-box-pack{width: 45rpx;}